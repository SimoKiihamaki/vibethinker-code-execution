{
  "name": "vibethinker-code-execution",
  "version": "1.0.0",
  "description": "MLX-powered agentic RAG system with Qwen3-VL-2B-Thinking, MCP, hooks, and skills for enhanced reasoning and repository analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "deploy": "./scripts/deploy.sh",
    "health-check": "python scripts/health_check.py",
    "convert-model": "python scripts/convert_model.py",
    "setup-mcp": "npm run build && node dist/mcp-server/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mlx",
    "qwen3",
    "qwen3-vl-2b-thinking",
    "mcp",
    "claude-code",
    "hooks",
    "skills",
    "rag",
    "repository-analysis",
    "progressive-disclosure",
    "agentic-ai",
    "multimodal-reasoning"
  ],
  "author": "VibeThinker",
  "license": "MIT",
