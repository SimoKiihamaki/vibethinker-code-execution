    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "ws": "^8.14.2",
    "zod": "^3.22.4",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
